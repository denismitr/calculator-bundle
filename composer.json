{
    "name": "denismitr/calculator-bundle",
    "description": "Calculator Bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Denis Mitrofanov",
            "email": "denis.mitr@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.9",
        "symfony/framework-bundle": "^4.2"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Denismitr\\CalculatorBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Denismitr\\CalculatorBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/simple-phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
